IMAPAccount com.matthewwynn.matthew
    Host imap.gmail.com
    User matthew@matthewwynn.com
    PassCmd "pass Email/apppass"
    SSLType IMAPS
    SSLVersions TLSv1.2

IMAPStore remote-com.matthewwynn.matthew
    Account com.matthewwynn.matthew

MaildirStore local-com.matthewwynn.matthew
    Path ~/.mail/com.matthewwynn.matthew/
    Inbox ~/.mail/com.matthewwynn.matthew/inbox

Channel com.matthewwynn.matthew
    Master :remote-com.matthewwynn.matthew:
    Slave :local-com.matthewwynn.matthew:
    Patterns * ![Gmail]* "[Gmail]/Sent Mail" "[Gmail]/Starred" "[Gmail]/All Mail"
    Create Both
    SyncState *

IMAPAccount edu.utdallas.mwynn
    Host webmail.utdallas.edu
    User maw140130
    PassCmd "pass School/utdallas.edu"
    PipelineDepth 1
    SSLType IMAPS
    SSLVersions TLSv1.2

IMAPStore remote-edu.utdallas.mwynn
    Account edu.utdallas.mwynn

MaildirStore local-edu.utdallas.mwynn
    Path ~/.mail/edu.utdallas.mwynn/
    Inbox ~/.mail/edu.utdallas.mwynn/inbox

Channel edu.utdallas.mwynn
    Master :remote-edu.utdallas.mwynn:
    Slave :local-edu.utdallas.mwynn:
    Patterns *
    Create Both
    SyncState *
    Remove Slave
    MaxMessages 40
    ExpireUnread no
