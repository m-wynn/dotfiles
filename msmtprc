defaults
# Use mail submission port 587
port            587
# Protocol to use
protocol        smtp
# Always use TLS
tls             on
# List of trusted CAs for TLS
tls_trust_file  /etc/ssl/certs/ca-certificates.crt
# Log file location
logfile         ~/.msmtp.log

account         com.matthewwynn.matthew
# Host name of SMTP server
host            smtp.gmail.com
# Envelope-from address
from            matthew@matthewwynn.com
# Authentication
auth            on
user            matthew@matthewwynn.com
passwordeval "pass Email/matthew@matthewwynn.com"
